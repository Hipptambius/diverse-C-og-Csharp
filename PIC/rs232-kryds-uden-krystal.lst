.................... BYTE read_high_temp() {        // Returns degrees F (0-255) 
....................    BYTE datah,datal; 
....................    signed long data; 
....................  
....................    i2c_start(); 
....................    i2c_write(0x90); 
....................    i2c_write(0xaa); 
....................    i2c_start(); 
....................    i2c_write(0x91); 
....................    datah=i2c_read(); 
....................    datal=i2c_read(0); 
....................    i2c_stop(); 
....................  
....................    data=datah; 
....................    data=data*9; 
....................    if(bit_test(datal,7)) 
....................    { 
....................       if(data < 0) 
....................           data -= 4; 
....................       else 
....................          data += 4; 
....................    } 
....................    data=(data/5)+32; 
....................  
....................    if(data < 0) 
....................       data = 0; 
....................    else if(data > 255) 
....................       data = 255; 
....................  
....................    return((int)data); 
.................... } 
